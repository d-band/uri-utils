{
  "name": "uri-utils",
  "version": "1.0.4",
  "description": "Utility for URI encoding based on RFC 3986.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js src test",
    "clean": "rimraf lib",
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha --no-timeouts --exit",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "benchmark": "node benchmark/index.js"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/uri-utils.git"
  },
  "keywords": [
    "URI",
    "RFC3986",
    "encodeURIComponent",
    "utility"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/uri-utils/issues"
  },
  "homepage": "https://github.com/d-band/uri-utils#readme"
}
