{
  "name": "uri-utils",
  "version": "1.0.3",
  "description": "Utility class for URI encoding based on RFC 3986.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js src test",
    "clean": "rimraf lib",
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha --no-timeouts --exit",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "benchmark": "node benchmark/index.js"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-env": "^1.1.4",
    "babel-register": "^6.18.0",
    "benchmark": "^2.1.4",
    "chai": "^4.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.0",
    "nyc": "^11.0.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/uri-utils.git"
  },
  "keywords": [
    "URI",
    "RFC3986",
    "encodeURIComponent",
    "utility"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/uri-utils/issues"
  },
  "homepage": "https://github.com/d-band/uri-utils#readme"
}
